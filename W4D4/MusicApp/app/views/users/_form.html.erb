<form action="/users" method="POST">
  <input 
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">
    
 <label>Email:
    <input 
      name="user[email]" 
      type="text">
 </label><br>
 
 <label>Password:
     <input 
     name="user[password]" 
     type="password">
 </label><br>
 
  <input type="submit">
</form>