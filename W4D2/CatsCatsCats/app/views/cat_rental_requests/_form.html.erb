<form action="<%= cat_rental_requests_url %>" method="POST">
  <input type="hidden"
           name="authenticity_token"
           value="<%= form_authenticity_token %>">
  <label>Cat:
    <select name="cat_rental_request[cat_id]">
      <% @cats.each do |cat| %>
      <option value="<%=cat.id%>">
        <%= cat.name %>
      </option>
      <% end %>
    </select><br>
    <label>Start Date:
        <input name="cat_rental_request[start_date]" type="date">
    </label><br>
    <label>End Date:
      <input name="cat_rental_request[end_date]" type="date">
  </label><br>
  <input type="submit">
</form>